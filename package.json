{
  "name": "multiforum",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 127.0.0.1",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "compile": "graphql-codegen",
    "lint": "eslint",
    "lint:a11y": "npx eslint \"components/**/*.vue\" --quiet",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "start": "nuxt start",
    "test": "cypress open",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest --coverage --ui",
    "coverage:open": "npm run coverage && open coverage/index.html",
    "knip": "knip",
    "tsc": "vue-tsc --noEmit",
    "verify": "npm run tsc && npm run test:unit:run",
    "lint-verify": "npm run test:unit:run && npm run lint:a11y"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@auth0/auth0-vue": "^2.4.0",
    "@google/model-viewer": "^4.1.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@headlessui/vue": "^1.7.23",
    "@mdi/font": "^7.4.47",
    "@monaco-editor/loader": "^1.7.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@sentry/nuxt": "^10.11.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vue/apollo-composable": "^4.2.2",
    "axios": "^1.11.0",
    "chart.js": "^4.5.1",
    "crypto-js": "^4.2.0",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^3.2.4",
    "dotenv": "^17.0.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "highlight.js": "^11.11.1",
    "identicon.js": "^2.3.3",
    "js-yaml": "^4.1.1",
    "jwt-decode": "^4.0.0",
    "lightgallery": "^2.8.3",
    "lucide-vue-next": "^0.548.0",
    "luxon": "^3.6.0",
    "markdown-it": "^14.1.0",
    "md-editor-v3": "^6.1.0",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.2",
    "three": "^0.172.0",
    "tslib": "^2.8.1",
    "v-code-diff": "^1.13.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-easy-lightbox": "^1.19.0",
    "vue-google-maps-community-fork": "^0.3.1",
    "vue3-popper": "^1.5.0",
    "vuedraggable": "^4.1.0",
    "vuemoji-picker": "^0.3.2",
    "vuetify": "3.7.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "^4.7.0",
    "@nuxt/components": "^2.2.1",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/schema": "^4.2.0",
    "@nuxt/types": "^2.18.1",
    "@nuxtjs/apollo": "^5.0.0-alpha.15",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@pinia/testing": "^1.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/diff-match-patch": "^1.0.36",
    "@types/google.maps": "^3.58.1",
    "@types/identicon.js": "^2.3.5",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.4.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.0",
    "@types/webpack-env": "^1.18.8",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "accented": "latest",
    "cypress": "^14.3.2",
    "eslint": "^9.36.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "globals": "^15.15.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.82.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.90.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.8"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix"
    ],
    "tests/**/*.spec.ts": [
      "npm run test:unit:run -- --run $1"
    ]
  }
}
